@using WebSampleMultipleDBTable.Models;

@model IEnumerable <Department>

@{
    ViewBag.Title = "Index";
}




<div style="font-family:Arial">
    <h2>Department List</h2>

    <ul>

        @foreach (Department department in Model)
        {
            <li>
                @Html.ActionLink(department.DeprtmentName, "HyperDetails", "Employee", new { departmentID = department.DepartmentID },null)
            </li>
        }

    </ul>

</div>

